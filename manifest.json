{
	"manifest_version": 3,
	"name": "ONTES Wizzard",
	"description": "A wizzard for the ONTES website",
	"version": "1.0",
	"icons": {
		"16": "/images/ontes-16.png",
		"48": "/images/ontes-48.png",
		"128": "/images/ontes-128.png"
	},
	"content_scripts": [
		{
			"matches": ["https://ontes.vsps-su.cz/*"],
			"js": ["ontes.js"]
		}
	],
	"permissions": ["activeTab"]
}
